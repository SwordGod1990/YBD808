/* pages/company/offer/offer.wxss */
@import "/template/toast/toast.wxss";

.toast-view {
    height: 100vh;
    width: 100%;
    overflow-y: hidden;
    position: fixed;
    z-index: 10;
    top: 0;
    background: rgba(0, 0, 0, 0.40);
}

page {
    width: 100%;
    background: #f2f2f7;
}

/* .offer-container{
    height: calc(100vh -100rpx);
} */

.tab-bar-view {
    background: white;
    display: flex;
    width: 100%;
    height: 100rpx;
    color: #666;
}

.tab-bar-view text {
    flex: 1;
    display: block;
    text-align: center;
    line-height: 100rpx;
    font-size: 30rpx;
    position: relative;
}

.select-txt {
    color: #eb3e2d;
}

.select-txt::after {
    content: '';
    background: #eb3e2d;
    width: 54rpx;
    height: 6rpx;
    position: absolute;
    bottom: 0;
    left: 50%;
    margin-left: -27rpx;
}

.content-view {
    display: flex;
}

.left-view {
    width: 182rpx;
    display: inline-block;
    height: calc(100vh - 200rpx);
    border-top-right-radius: 6px;
}

.left-view-content {
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.left-item-view {
    padding-right: 15rpx;
    padding-bottom: 10rpx;
    border-bottom: 1rpx solid #e5e5e5;
    position: relative;
    color: #333;
    background: white;
}

.left-item-view2 {
    padding-right: 15rpx;
    padding-bottom: 15rpx;
    border-bottom: 1rpx solid #e5e5e5;
    position: relative;
    color: #333;
    background: white;
}

.left-item-select {
    background: #f2f2f7;
    color: #eb3e2d;
}

.right-view {
    display: inline-block;
    height: calc(100vh - 200rpx);
    width: 70.4%;
    margin-left: 20rpx;
}

.company-name {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 24rpx;
    padding-left: 15rpx;
    padding-top: 18rpx;
}

.company-name2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding-left: 15rpx;
    padding-top: 18rpx;
    font-size: 30rpx;
    /* padding-bottom: 15rpx; */
    word-break: break-all;
}

.mount-txt {
    font-size: 24rpx;
    /* color: #333; */
    padding-left: 15rpx;
    vertical-align: middle;
}

.left-view image {
    width: 52rpx;
    height: 52rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.medicina-name-txt{
    white-space: nowrap;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    padding-right: 100rpx;
}

.freight {
    font-size: 26rpx;
    color: #666;
    /* height: 100rpx; */
    background: #fff;
    box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.10);
    border-radius: 6px;
    padding-left: 28rpx;
    padding-top: 8rpx;
    padding-bottom: 15rpx;
    padding-right: 15rpx;
}

.freight text {
    display: block;
    padding-top: 8rpx;
    /* white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis; */
}

.drug-item-view {
